{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "autoAttachChildProcesses": true,
      "args": [
        "${workspaceFolder}/readme.md",
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "skipFiles": ["<node_internals>/**", "**/node_modules/vscode-*/**"],
      "preLaunchTask": "build"
    }
  ]
}
